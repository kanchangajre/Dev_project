node {
    def mvnPath
    
    stage('git clone') {
        git 'https://github.com/kanchangajre/dev_project/tree/master.git'
    }
    
    stage('Maven build'){
        def mvnHome = tool name:'maven', type:"maven";
        mvnPath = "${mvnHome}/bin/mvn";
        sh "${mvnPath} clean package"
    }    
}
