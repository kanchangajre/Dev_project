pipeline {
    agent any
    
    tools{
        maven "Maven-3.9.4"
    }

    stages {
        stage('Clone') {
            steps {
               git 'https://github.com/kanchangajre/dev_project/tree/master.git'
            }
        }
         stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
